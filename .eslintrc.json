{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:sonarjs/recommended", "prettier"],
  "plugins": ["prettier", "sonarjs"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error"],
    "quotes": [2, "single"],
    "react/prop-types": [0]
  }
}
